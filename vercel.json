{
  "version": 2,
  "builds": [
    {
      "src": "frontend/next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://mgnrega-beta.vercel.app",
    "NEXT_PUBLIC_ENABLE_GEOLOCATION": "true",
    "NODE_ENV": "production"
  }
}